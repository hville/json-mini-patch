{
  "name": "json-mini-patch",
  "version": "2.0.0",
  "description": "small minimal JSON patch implementation",
  "keywords": [
    "JSON",
    "JSON patch",
    "rfc 6902"
  ],
  "author": "Hugo Villeneuve",
  "main": "./dist/index.js",
  "module": "./module.js",
  "browser": "./dist/browser.js",
  "dependencies": {},
  "devDependencies": {
    "cotest": "^1.6.1",
    "json-patch-test-suite": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hville/json-mini-patch.git"
  },
  "scripts": {
    "prepublish": "npm test",
    "test": "npm run build:main & cotest ./tst",
    "gzip": "node -e \"fs.writeFileSync(process.argv[2], zlib.gzipSync(fs.readFileSync(process.argv[1])))\"",
    "build:main": "rollup -o ./dist/index.js -f cjs --banner \"/* hugov@runbox.com | https://github.com/hville/json-mini-patch.git | license:MIT */\" ./module.js",
    "build:browser": "rollup -o ./dist/browser.js -f iife -n json-mini-patch --banner \"/* hugov@runbox.com | https://github.com/hville/json-mini-patch.git | license:MIT */\" ./module.js",
    "build:min": "google-closure-compiler-js --compilationLevel ADVANCED --languageIn ES5 --languageOut ES5 --useTypesForOptimization true ./dist/browser.js > ./dist/browser.min.js",
    "build:gzip": "npm run gzip -- ./dist/browser.min.js ./bld/browser.gz",
    "build": "npm run build:main & npm run build:module & npm run build:browser & npm run build:min",
    "prepublishOnly": "npm test & npm run build:browser & npm run build:min"
  },
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
